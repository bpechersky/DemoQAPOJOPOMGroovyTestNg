<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DemoQA Suite">
    <listeners>
        <listener class-name="com.demoqa.listeners.TestListener"/>
    </listeners>
    <test name="All Tests Except ProgressBar">
        <classes>
            <class name="com.demoqa.tests.TextBoxTest"/>
            <class name="com.demoqa.tests.CheckBoxTest"/>
            <class name="com.demoqa.tests.RadioButtonTest"/>
            <class name="com.demoqa.tests.PracticeFormTest"/>
            <class name="com.demoqa.tests.BrowserWindowsTest"/>
            <!-- Do NOT include ProgressBarTest -->
        </classes>
    </test>
</suite>
